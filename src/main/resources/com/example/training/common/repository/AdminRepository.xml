<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper
	namespace="com.example.training.common.repository.AdminRepository">
	<select id="findByName"
		resultType="com.example.training.web.domain.admin.Admin">
		SELECT
		*
		FROM
		ADMIN
		WHERE
		name = "${name}"
	</select>

</mapper>
